                        <i class="fab fa-twitter fa-stack-1x"></i>
                    </a>
                </span>
            </span> --}}
        </div>
    </nav> <!-- end of navbar -->
    <!-- end of navigation -->


    <!-- Header -->
    <header id="header" class="header">
        <div class="header-content">
            <div class="container">
                    <div class="row">
                        <div class="container">
                                <div class="col-lg-12">
                                <div class="btn-group btn-group-justified">
                                        <button type="button" class="btn btn-primary" data-micromodal-trigger="modal-1" >Create Newboard</button>
                                        <button type="button" class="btn btn-primary">Account Settings</button>
                                        <button type="button" class="btn btn-primary">Logout</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                          
                    <div class="col-lg-12">
 
                        <div class="tools">
                          <input type="search" required placeholder="Notice Board Url"  name="search" id="search">
                         <button id="searchbtn"> CHECK AVAILABILITY </button>
                         <span id="notify"></span>
                         <input id="key" type="hidden"  name="_csrf" value="{{csrfToken}}">
                     
                         <div class="ff">
                                 <input type="text" required name="title" id="title" placeholder="Notice Board Title">
                      
                         </div>
                         <div class="ff">
                                 <input type="number" required name="number" id="number" placeholder="Contact Number">
                      
                         </div>
                          <div class="ff">
                                 <input type="email" required name="email" id="conemail" placeholder="Contact Email">
                      
                         </div>
                         <div class="ff">
                             <button id="submitNow"> CREATE NOW </button>
                         </div>

                        </div>
                        
                    
                   </div>
                    </div> <!-- end of col -->

                    </div>
            </div>
                  
                </div> <!-- end of row -->
            </div> <!-- end of container -->
        </div> <!-- end of header-content -->
    </header> <!-- end of header -->
    <!-- end of header -->
